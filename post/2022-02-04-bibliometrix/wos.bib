@article{ WOS:000423802600005,
Author = {Kleinberg, Jon and Lakkaraju, Himabindu and Leskovec, Jure and Ludwig,
   Jens and Mullainathan, Sendhil},
Title = {{HUMAN DECISIONS AND MACHINE PREDICTIONS}},
Journal = {{QUARTERLY JOURNAL OF ECONOMICS}},
