Title = {{Observational Data for Heterogeneous Treatment Effects with Application
   to Recommender Systems}},
Booktitle = {{ACM EC `19: PROCEEDINGS OF THE 2019 ACM CONFERENCE ON ECONOMICS AND
   COMPUTATION}},
Year = {{2019}},
