@article{ WOS:000541977600001,
Author = {Wang, Haoran and Zhou, Xun Yu},
Title = {{Continuous-time mean-variance portfolio selection: A reinforcement
   learning framework}},
Journal = {{MATHEMATICAL FINANCE}},
Year = {{2020}},
Volume = {{30}},
Number = {{4}},
Pages = {{1273-1308}},
Month = {{OCT}},
Abstract = {{We approach the continuous-time mean-variance portfolio selection with
   reinforcement learning (RL). The problem is to achieve the best
   trade-off between exploration and exploitation, and is formulated as an
   entropy-regularized, relaxed stochastic control problem. We prove that
   the optimal feedback policy for this problem must be Gaussian, with
   time-decaying variance. We then prove a policy improvement theorem,
   based on which we devise an implementable RL algorithm. We find that our
   algorithm and its variant outperform both traditional and deep neural
   network based algorithms in our simulation and empirical studies.}},
