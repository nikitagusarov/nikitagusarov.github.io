@inproceedings{ WOS:000343127000004,
Author = {van Bergem, Rutger and Jenkins, Jeffrey and Benachenhou, Dalila and Szu,
   Harold},
Editor = {{Szu, HH and Dai, L}},
Title = {{NON-EQUILIBRIUM THERMODYNAMICS THEORY OF ECONOMETRIC SOURCE DISCOVERY
   FOR LARGE DATA ANALYSIS}},
Booktitle = {{INDEPENDENT COMPONENT ANALYSES, COMPRESSIVE SAMPLING, WAVELETS, NEURAL
   NET, BIOSYSTEMS, AND NANOENGINEERING XII}},
Series = {{Proceedings of SPIE}},
