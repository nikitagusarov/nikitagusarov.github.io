Title = {{A new evidential K-nearest neighbor rule based on contextual discounting
   with partially supervised learning}},
Journal = {{INTERNATIONAL JOURNAL OF APPROXIMATE REASONING}},
Year = {{2019}},
Volume = {{113}},
Pages = {{287-302}},
