@article{ WOS:000693728100001,
Author = {Ellingsen, Jon and Larsen, Vegard H. and Thorsrud, Leif Anders},
Title = {{News media versus FRED-MD for macroeconomic forecasting}},
Journal = {{JOURNAL OF APPLIED ECONOMETRICS}},
Abstract = {{Using a unique dataset of 22.5 million news articles from the Dow Jones
   Newswires Archive, we perform an in depth real-time out-of-sample
   forecasting comparison study with one of the most widely used data sets
   in the newer forecasting literature, namely the FRED-MD dataset.
   Focusing on US GDP, consumption and investment growth, our results
   suggest that the news data contains information not captured by the hard
   economic indicators, and that the news-based data are particularly
   informative for forecasting consumption developments.}},
