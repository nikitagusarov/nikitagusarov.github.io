@article{ WOS:000381561700005,
Author = {Hagiwara, Junichiro},
Title = {{Real-Time Joint Channel and Hyperparameter Estimation Using Sequential
   Monte Carlo Methods for OFDM Mobile Communications}},
Journal = {{IEICE TRANSACTIONS ON COMMUNICATIONS}},
