@article{ WOS:000600940700005,
Author = {Iskhakov, Fedor and Rust, John and Schjerning, Bertel},
Title = {{Machine learning and structural econometrics: contrasts and synergies}},
Journal = {{ECONOMETRICS JOURNAL}},
