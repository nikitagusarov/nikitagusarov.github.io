@inproceedings{ WOS:000381554504032,
Author = {Mattila, Robert and Krishnamurthy, Vikram and Wahlberg, Bo},
Book-Group-Author = {{IEEE}},
Title = {{Recursive Identification of Chain Dynamics in Hidden Markov Models Using
   Non-Negative Matrix Factorization}},
