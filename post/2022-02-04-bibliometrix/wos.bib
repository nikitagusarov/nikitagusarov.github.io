@article{ WOS:000714918800023,
Author = {Wu, Liming},
Title = {{ENTROPICAL OPTIMAL TRANSPORT, SCHRODINGER'S SYSTEM AND ALGORITHMS}},
Journal = {{ACTA MATHEMATICA SCIENTIA}},
