@article{ WOS:000359070000012,
Author = {Kreif, Noemi and Grieve, Richard and Diaz, Ivan and Harrison, David},
Title = {{Evaluation of the Effect of a Continuous Treatment: A Machine Learning
   Approach with an Application to Treatment for Traumatic Brain Injury}},
Journal = {{HEALTH ECONOMICS}},
